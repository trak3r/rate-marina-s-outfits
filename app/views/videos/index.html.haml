#videos
  - @videos.each do |video|
    .thumbnail
      = link_to_remote(image_tag(video.thumbnail_url, :alt => h(video.title)), :method=> 'get', :update => 'content', :url => video_url(video))
      = ratings_for(video, :static)
#pagination
  = will_paginate(@videos, :renderer => 'RemoteLinkRenderer' , :remote => {:update => 'content'} )
